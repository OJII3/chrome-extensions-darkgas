{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Dark Theme in Gas",
    "short_name": "DarkGAS",
    "description": "An extention to use dark theme in Google Apps Script",
    "action": {
        "default_icon": "./miku.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://script.google.com/home/*"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ]
}